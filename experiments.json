{
  "bookmark-history-menu": {
    "match": {
      "appId": "^org.mozilla.fennec|^org.mozilla.firefox_beta$"
    },
    "buckets": {
      "min": "0",
      "max": "100"
    }
  },
  "offline-cache": {
    "match": {
      "appId": "^org.mozilla.fennec|^org.mozilla.firefox_beta$"
    },
    "buckets": {
      "min": "0",
      "max": "100"
    }
  },
  "search-term": {
    "match": {
      "appId": "^org.mozilla.fennec|^org.mozilla.firefox_beta$"
    },
    "buckets": {
      "min": "0",
      "max": "100"
    }
  },
  "whatsnew-notification": {
    "match": {
    },
    "buckets": {
      "min": "0",
      "max": "0"
    }
  },
  "content-notifications-12hrs": {
    "match": {
      "appId": "^org.mozilla.fennec"
    },
    "buckets": {
      "min": "25",
      "max": "50"
    }
  },
  "content-notifications-8am": {
    "match": {
      "appId": "^org.mozilla.fennec"
    },
    "buckets": {
      "min": "50",
      "max": "75"
    }
  },
  "content-notifications-5pm": {
    "match": {
      "appId": "^org.mozilla.fennec"
    },
    "buckets": {
      "min": "75",
      "max": "100"
    }
  },
  "promote-add-to-homescreen": {
    "match": {
      "appId": "^org.mozilla.fennec$"
    },
    "buckets": {
      "min": 50,
      "max": 100
    },
    "values": {
      "minimumTotalVisits": 5,
      "lastVisitMinimumAgeMs": 30000,
      "lastVisitMaximumAgeMs": 600000
    }
  } 
}
